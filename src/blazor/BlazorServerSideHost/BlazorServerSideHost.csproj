<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>
		<LangVersion>Preview</LangVersion>
		<RootNamespace>Super.Paula.Client</RootNamespace>
		<UserSecretsId>b60582a0-a9df-4614-a837-b9ab58e3463d</UserSecretsId>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\BlazorApp\BlazorApp.csproj" />
		<PackageReference Include="Microsoft.Extensions.Configuration.KeyPerFile" Version="6.0.2" />
	</ItemGroup>

	<Target Name="PreBuild" BeforeTargets="PreBuildEvent">
	  <Exec Command="git rev-parse --abbrev-ref HEAD &gt; &quot;$(TargetDir)build__branch&quot;&#xD;&#xA;git rev-parse HEAD &gt; &quot;$(TargetDir)build__hash&quot;&#xD;&#xA;git rev-parse --short HEAD &gt; &quot;$(TargetDir)build__shorthash&quot;" />
	</Target>

</Project>
